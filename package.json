{
  "name": "myapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "babel-plugin-relay": "^2.0.0-rc.2",
    "flow-bin": "^0.96.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "material-ui": "^0.20.2",
    "prettier": "^1.16.4",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-responsive-carousel": "^3.1.47",
